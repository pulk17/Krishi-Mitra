{
  "name": "krishi-mitra",
  "version": "1.0.0",
  "description": "AI-powered plant disease diagnosis system",
  "private": true,
  "scripts": {
    "install:all": "pnpm install",
    "dev": "pnpm --filter \"./packages/*\" --parallel dev",
    "dev:backend": "pnpm --filter krishi-mitra-backend dev",
    "dev:frontend": "pnpm --filter plant-diagnosis-frontend dev",
    "build:frontend": "pnpm --filter \"plant-diagnosis-frontend^...\" build",
    "build:backend": "pnpm --filter \"krishi-mitra-backend^...\" build",
    "clean": "rimraf \"node_modules\" \"packages/**/node_modules\" \"packages/**/dist\" \"packages/**/.next\" \"packages/**/*.tsbuildinfo\"",
    "db:pull": "prisma db pull --schema=./packages/backend/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./packages/backend/prisma/schema.prisma"
  },
  "keywords": [
    "agriculture",
    "ai",
    "plant-disease",
    "diagnosis"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv-cli": "^10.0.0",
    "next": "^15.5.3",
    "prisma": "^6.16.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1"
  }
}