{
  "name": "krishi-mitra-backend",
  "version": "1.0.0",
  "description": "Express.js backend for Krishi Mitra plant diagnosis system",
  "main": "dist/index.js",
  "scripts": {
    "predev": "prisma generate",
    "dev": "dotenv -e ../../.env -- tsx --watch src/index.ts",
    "test:gemini": "tsx test-gemini.ts",
    "build": "prisma generate && tsc --build --verbose && cpr src/python dist/python -o",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "postinstall": "prisma generate",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@krishi-mitra/types": "workspace:*",
    "@prisma/client": "^5.15.0",
    "@supabase/supabase-js": "^2.43.4",
    "@types/multer": "1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "1.4.5-lts.1",
    "pm2": "^5.4.2",
    "prisma": "^5.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "cpr": "^3.0.1",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "express",
    "typescript",
    "ai",
    "plant-diagnosis"
  ],
  "author": "PseudoCoders",
  "license": "MIT"
}